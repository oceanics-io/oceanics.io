{
  "compilerOptions": {
    "incremental": true,
    "target": "esnext",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": false,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "declaration": true,
    "sourceMap": true,
    "rootDir": ".",
    "removeComments": true,
    "jsx": "preserve",
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
  "include": [
    "next-env.d.ts",
    "src/**/*.ts",
    "src/**/*.tsx",
    "src/**/*.json",
    "src/**/*.js",
    ".next/types/**/*.ts",
    "build/types/**/*.ts"
, "app/Index.tsx", "app/canvas/Account.tsx", "app/canvas/Account.worker.ts", "src/components/References/Document.tsx", "app/canvas/data-stream/DataStream.tsx", "app/canvas/data-stream/useDataStream.tsx", "app/catalog/PopUpContent.tsx", "app/catalog/Squalltalk.tsx", "app/catalog/Squalltalk.worker.ts", "app/catalog/useSqualltalk.tsx", "app/canvas/data-stream/Histogram.tsx", "app/canvas/data-stream/worker.ts", "app/canvas/data-stream/useHistogram.tsx"  ],
  "exclude": [
    "node_modules",
    "src/components/Simulation/Shaders.context.tsx",
    ".next"
  ]
}
