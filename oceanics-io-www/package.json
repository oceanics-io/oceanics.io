{
  "name": "oceanics-io-www",
  "description": "Blue computing",
  "version": "0.2.0",
  "author": "Oceanicsdotio <business@oceanics.io>",
  "dependencies": {
    "@babel/core": "^7.13.14",
    "@mdx-js/mdx": "^1.6.21",
