    "gatsby-cli": "^3.10.0",
    "gatsby-image": "^3.10.0",
    "gatsby-plugin-manifest": "latest",
    "gatsby-plugin-mdx": "^2.10.0",
    "gatsby-plugin-react-helmet": "latest",
    "gatsby-plugin-sharp": "2.14.3",
    "gatsby-remark-copy-linked-files": "latest",
    "gatsby-remark-images": "latest",
    "gatsby-remark-katex": "latest",
    "gatsby-remark-prismjs": "^3.13.0",
    "gatsby-remark-responsive-iframe": "^2.11.0",
    "gatsby-remark-smartypants": "^2.10.0",
    "gatsby-source-filesystem": "latest",
    "gatsby-transformer-remark": "latest",
    "gatsby-transformer-sharp": "^2.12.1",
    "katex": "^0.13.11",
    "oceanics-io-ui": "workspace:*",
    "prismjs": "^1.24.1",
    "react": "16.14.0",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-is": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "react-typography": "^0.16.19",
    "remark-parse": "^9.0.0",
    "typography": "^0.16.19"
  },
  "devDependencies": {
    "babel-loader": "^8.2.2",
    "eslint": "^7.23.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.2.1"
  },
  "homepage": "https://www.oceanics.io",
  "keywords": [
    "ocean data",
    "water quality",
    "remote sensing",
    "simulation",
    "web assembly"
  ],
  "license": "MIT",
  "main": "n/a",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/oceanicsdotio/oceanics.io.git"
  },
  "scripts": {
    "develop": "GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "build": "make build",
    "develop-no-cache": "rm -rf .cache && GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "start:fcns": "netlify-lambda serve functions -c ./webpack.config.js",
    "build:fcns": "netlify-lambda build functions -c ./webpack.config.js"
  },
  "proxy": {
    "/.netlify/functions": {
      "target": "http://localhost:9000",
      "pathRewrite": {
        "^/\\.netlify/functions": ""
      }
    }
  }
}
